    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.9.0",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.11.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.0"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "node_modules"
    ],
    "rootDir": "src"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": true
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.8",
