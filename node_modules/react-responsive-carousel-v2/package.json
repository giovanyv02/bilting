{
  "name": "react-responsive-carousel-v2",
  "version": "3.2.0",
  "description": "React Responsive Carousel v2",
  "author": {
    "name": "Isaac Pak"
  },
  "main": "./dist/index.js",
  "license": "MIT",
  "keywords": [
    "react",
    "carousel",
    "gallery",
    "image-gallery",
    "slider",
    "responsive",
    "swipe",
    "mobile-friendly",
    "react-component",
    "view"
  ],
  "engines": {
    "node": ">=4"
  },
  "types": "./index.d.ts",
  "scripts": {
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "fserver": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --config webpack.config.js --content-base dist --host 0.0.0.0 --port 9000 --inline --env development",
    "pretranspile": "rimraf dist",
    "transpile": "babel src -d dist --copy-files",
    "prepublishOnly": "npm run transpile",
    "prod": "npm run webpack -- --mode production",
    "dev:debug": "npm run debug -- --mode development",
    "prod:debug": "npm run debug -- --mode production",
    "update-snapshots": "jest --updateSnapshot",
    "prepublish-to-npm": "git pull && npm run build && git add . && git commit -m 'Prepare for publishing'",
    "publish-to-npm": "(git pull origin master && npm version patch && git push origin master && npm publish && git push --tags)",
    "postpublish-to-npm": "npm run changelog && git add . && git commit -m 'Updating changelog' && git push origin master",
    "storybook": "start-storybook -p 9001 -s ./src -c .storybook",
    "build-storybook": "build-storybook -s ./src -o ./dist/storybook",
    "changelog": "auto-changelog --ignore-commit-pattern=\"(Merge pull request|Merge branch|Updating changelog|Prepare for publishing)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pak11273/react-responsive-carousel-v2.git"
  },
  "bugs": {
    "url": "https://github.com/pak11273/react-responsive-carousel-v2/issues"
  },
  "homepage": "http://pak11273.github.io/react-responsive-carousel-v2/",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
